<script>
  // Detectar el idioma del navegador
  var userLang = navigator.language || navigator.userLanguage;

  // Redirigir al subdirectorio correspondiente
  if (userLang.startsWith('es')) {
      window.location.href = "/es/";
  } else {
      window.location.href = "/en/";
  }
</script>>

